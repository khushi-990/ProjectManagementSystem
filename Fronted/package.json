{
  "name": "building-bridges-communication-volunteer-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "test": "vitest run",
    "test:watch": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "coverage": "vitest run --coverage ",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write \"**/*.{ts,tsx,css}\" --config ./.prettierrc",
    "prepare": "husky && husky install",
    "format": "prettier --write \"**/*.{ts,tsx,css}\" --config ./.prettierrc",
    "tsc": "tsc"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@tanstack/react-query": "^4.33.0",
    "@tanstack/react-query-devtools": "^4.33.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@vitejs/plugin-react": "^4.0.4",
    "antd": "^5.10.1",
    "axios": "^1.4.0",
    "dayjs": "^1.11.10",
    "memoize": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-phone-number-input": "^3.2.16",
    "react-router-dom": "^6.15.0",
    "serve": "^14.2.0",
    "simplebar-react": "^3.2.4",
    "socket.io-client": "^4.7.5",
    "styled-components": "^6.0.8",
    "typescript": "^5.2.2",
    "vite-plugin-pwa": "^0.16.5",
    "vite-tsconfig-paths": "^4.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.4",
    "workbox-window": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint",
      "npm run prettier"
    ],
    "*.{js,css,md,html,json}": "prettier --cache --write"
  }
}
